{
    "openapi": "3.0.3",
    "info": {
        "title": "Authentication API",
        "description": "Authentication API",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:8080"
        }
    ],
    "paths": {
        "/api/auth/login": {
            "post": {
                "description": "Login User",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "examples": {
                                "test": {
                                    "description": "login user test",
                                    "value": {
                                        "email": "test@example.com",
                                        "password": "password"
                                    }
                                }
                            },
                            "schema": {
                                "required": ["password", "email"],
                                "type": "object",
                                "properties": {
                                    "email": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Login Success",
                        "content": {
                            "application/json": {
                                "example": {
                                    "success": true,
                                    "data": {
                                        "user": {
                                            "name": "test",
                                            "email": "test@example.com",
                                            "access_token": "5|1Zx8SC8PhS4zaB8eikrdFmFoK8z5Y5jCU2fCC5dx839b5258"
                                        }
                                    }
                                },
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "type": "object",
                                            "properties": {
                                                "success" : {
                                                    "type": "boolean"
                                                },
                                                "data" : {
                                                    "type": "object",
                                                    "properties": {
                                                        "user": {
                                                            "type": "object",
                                                            "properties": {
                                                                "name": {
                                                                    "type": "string"
                                                                },
                                                                "email": {
                                                                    "type": "string"
                                                                },
                                                                "access_token": {
                                                                    "type": "string"
                                                                }
                                                            }
                                                        }
                                                    }
                                                }
                                            }
                                        },
                                        "errors": {
                                            "type": "object"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/auth/logout": {
            "post": {
                "description": "Logout",
                "parameters": [
                    {
                        "name": "Authorization",
                        "description": "put on bearer token",
                        "in": "header",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "204": {
                        "description": "Logout success"
                    }
                }
            }
        },
        "/api/auth/register": {
            "post": {
                "description": "create new user",
                "parameters": [
                    {
                        "name": "Authorization",
                        "description": "put on bearer token",
                        "in": "header",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "example": {
                                "name": "Husni Robani",
                                "email": "husni@example.com",
                                "password": "password",
                                "password_confirmation": "password"
                            },
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "email": {
                                        "type": "string",
                                        "pattern": "email"
                                    },
                                    "password": {
                                        "type": "string"
                                    },
                                    "password_confirmation": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "success to create user",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "status": {
                                            "type": "string"
                                        },
                                        "message": {
                                            "type": "string"
                                        },
                                        "data": {
                                            "type": "object",
                                            "properties": {
                                                "user": {
                                                    "type": "object"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "unauthorized for not admin user",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "status": {
                                            "type": "string"
                                        },
                                        "message": {
                                            "type": "string"
                                        },
                                        "error": {
                                            "type": "object"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "query exception or another error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "status": {
                                            "type": "string"
                                        },
                                        "message": {
                                            "type": "string"
                                        },
                                        "error": {
                                            "type": "object"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
